<h3>SUMMARY</h3>
<table>
  <tr>
    <th>HEADING</th>
    <th>ELEMENT</th>
    <th>START</th>
    <th>FINISH</th>
    <th>OPERATION</th>
    <th helvetica="3"></th>
  </tr>

            <% @inspections.each do |inspection| %>
            <tr>
              <td><%= inspection.heading %></td>
              <td><%= inspection.element %></td>
              <td><%= inspection.start %></td>
              <td><%= inspection.finish %></td>
              <td><%= inspection.operation %></td>
              <td><%= image_tag inspection.image.url(:thumb) %></td>
            </tr>
          <% end -%>

<% @walls.each do |wall| %>
  <tr>
    <td><%= wall.heading %></td>
    <td><%= wall.element %></td>
    <td><%= wall.start %></td>
    <td><%= wall.finish %></td>
    <td><%= wall.operation %></td>
  </tr>
   <% end -%>
   <% @slabs.each do |slab| %>
  <tr>
    <td><%= slab.heading %></td>
    <td><%= slab.element %></td>
    <td><%= slab.start %></td>
    <td><%= slab.finish %></td>
    <td><%= slab.operation %></td>
  </tr>
  <% end -%>
  </table>