<div class = "text-center" id = "panel" style=" display: none;">

    <h5>Inspections</h5>
<br>
          <table class = "table">
            <tr>

              <th>Heading</th>
              <th>Element</th>
              <th>Start</th>
              <th>Finish</th>
              <th >Scope</th>
              <th>Operation</th>
              <th>Notes</th>
              <th>Image</th>
              <th>Erase</th>
              
            </tr>
            <% @inspection_report.inspections.reverse_each do |inspection| %>
              <tr>

                <td><%= inspection.heading %></td>
                <td><%= inspection.element %></td>
                <td><%= inspection.start %></td>
                <td><%= inspection.finish %></td>
                <td><%= inspection.scope %></td>
                <td><%= inspection.operation %></td>
                <td><%= inspection.note %></td>
                 

                <td class = "image"><%= image_tag inspection.image.url(:thumb)  %></td>

                <td><strong><%= button_to 'o', [inspection.inspection_report, inspection],
                           method: :delete,:class => "btn btn-link" 
                           %><strong></td>
                           
                
              </tr>
            <% end %>
          </table>

<div>
    <% @inspection_report.inspections.reverse_each do |inspection| %>
      <tr>
    <td><%= image_tag inspection.image.url(:original) %></td>
  </tr>
<% end -%>

</div>

<div class = "text-center" id = "panel_hours" style=" display: none;" >

    <h5>Manhours</h5>
    <br>
    <table class = "table">
            <tr>
              <th>Heading</th>
              <th>Union - 147</th>
              <th>Union - 15</th>
              <th>Union - 14</th>
              <th>Union - 731</th>     
              <th>Union - 3</th>
              <th>Erase</th>
            </tr>

            <% @inspection_report.forces.each do |force| %>
              <tr>
                <td><%=force.heading %></td>
                <td><%=force.union147 %></td>
                <td><%=force.union15 %></td>
                <td><%=force.union14 %></td>
                <td><%=force.union731 %></td>
                <td><%=force.union3 %></td>

            <td><strong><%= button_to 'o', [force.inspection_report, force],
                           method: :delete,:class => "btn btn-link" 
                           %><strong></td>

              </tr>
            <% end %>


          </table>
</div>

<div class="text-center" id = "panel_obs" style=" display: none;">

    <h5>Observations</h5>
    <br>
    <table>
          <tr>
            <% @inspection_report.notes.each do |note| %>
            <%= note.observation %>
            <%= button_to 'o', [note.inspection_report, note],
            method: :delete ,:class => "btn btn-link",
            data: { confirm: 'Are you sure?' } %>
          </tr>
      <% end %>
    </table>
 </div>
</div>



